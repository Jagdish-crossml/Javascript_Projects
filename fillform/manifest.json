{
    "manifest_version": 3,
    "name": "fill Form extension",
    "description": "automatically fill forms of a user",
    "homepage_url": "http://127.0.0.1:5503/ext_form.html",
    "version": "1.0",
    "action": {
        "default_title": "Click here to load form data!",
        "default_popup": "suggestion.html"
    },
    "background": {
        "service_worker": "service.js"
    },
    "content_scripts": [
        {
            "matches": [
                "http://127.0.0.1:5503/ext_form.html",
                "http://127.0.0.1:5503/testing_form.html"
            ],
            "js": [
                "jquery-3.6.0.js"
            ],
            "css": [
                "suggest/css/login.css",
                "suggest/css/wepage9.css"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "index.html",
                "suggest/suggestion.html",
                "suggest/wepage9.html",
                "suggest/appointment.html",
                "suggest/wepage9.js",
                "suggest/css/appointment.css"
            ],
            "matches": [
                "http://127.0.0.1:5503/*"
            ]
        }
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "permissions": [
        "storage",
        "tabs",
        "activeTab",
        "scripting"
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'none'",
        "content_scripts": "script-src 'self'; object-src 'none'"
    }
}