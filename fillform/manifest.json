{
    "manifest_version": 2,
    "name": "fill Form extension",
    "description": "automatically fill forms of a user",
    "homepage_url": "http://127.0.0.1:5503/ext_form.html",
    "version": "1.0",
    "browser_action": {
        "default_title": "Click here to load form data!",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_scripts": [
        {   "matches":["http://127.0.0.1:5503/ext_form.html","http://127.0.0.1:5503/testing_form.html"],
            "js": ["jquery-3.6.0.js","content.js"],
            "css":["suggest/css/login.css","suggest/css/wepage9.css"]
        }
    ],
    "web_accessible_resources": [
        "index.html",
        "suggest/suggestion.html",
        "suggest/wepage9.html"
       
    ],
    "permissions": [
        "storage",
        "tabs",
        "http://*/*",
        "https://*/*",
        "activeTab"
    ]
}